[user]
	email = liberty195@gmail.com
	name = redVi
[push]
	default = matching
[help]
	autocorrect = 1
[core]
	editor = nano
	autocrlf = input
  quotepath = false
  whitespace = cr-at-eol,indent-with-non-tab,blank-at-eol,space-before-tab
[color]
	ui = true
[alias]
  cb = checkout -b
  cm = commit -m
  cam = commit -a -m
  st = status
  di = diff
  als = config --global --get-regexp alias
[merge]
	tool = diffuse
